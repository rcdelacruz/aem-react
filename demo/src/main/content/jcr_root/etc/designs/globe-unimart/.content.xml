            jcr:primaryType="nt:unstructured"
            sling:resourceType="globe-unimart/components/accordion"
            components="globe-unimart/components/accordion-element">
            <section jcr:primaryType="nt:unstructured"/>
         </accordion>

        <page jcr:primaryType="nt:unstructured">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys"
                components="[group:Globe Unimart, group:General]">
                <section jcr:primaryType="nt:unstructured"/>
             </par>
         </page>
    </jcr:content>
</jcr:root>
